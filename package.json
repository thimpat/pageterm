{
  "name": "pageterm",
  "version": "1.0.0",
  "description": "Display help in a terminal",
  "main": "cjs/index.cjs",
  "bin": {
    "pageterm": "cli.cjs"
  },
  "scripts": {
    "build:esm": "to-esm cjs/index.cjs --output esm/ --subRootDir cjs --update-all --keep-external",
    "build:demo": "to-esm demo/demo.cjs --output demo/ --subRootDir demo --keep-external",
    "run:demo": "demo/demo.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thimpat/pageterm.git"
  },
  "keywords": [
    "terminal",
    "help",
    "display",
    "term"
  ],
  "author": "Patrice Thimothee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thimpat/pageterm/issues"
  },
  "homepage": "https://github.com/thimpat/pageterm#readme",
  "dependencies": {
    "@thimpat/libutils": "^1.5.1",
    "commander": "^9.4.0",
    "term-size": "^3.0.2",
    "to-ansi": "^1.4.3",
    "window-size": "^1.1.1"
  },
  "devDependencies": {
    "marked": "^4.1.0",
    "marked-terminal": "^5.1.1",
    "to-esm": "^2.19.0"
  },
  "exports": {
    "require": "./cjs/index.cjs",
    "import": "./esm/index.mjs"
  }
}